{
  "name": "epic-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:sync": "npm run typeorm schema:sync -- -d src/db/app-data-source.ts",
    "db:drop": "npm run typeorm schema:drop -- -d src/db/app-data-source.ts",
    "db:restore": "npm run db:drop && npm run db:sync",
    "test": "npx ts-node src/__test__/test.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.0.0-beta.34",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@next/env": "^15.1.2",
    "next": "14.1.3",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "socket.io-client": "^4.7.5",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
